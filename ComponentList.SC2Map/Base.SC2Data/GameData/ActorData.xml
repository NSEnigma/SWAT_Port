        <On Terms="Behavior.ZombieFlaming.On" Send="SetTintColor 255,150,81"/>
        <On Terms="Behavior.ZombieFlaming2.On" Send="SetTintColor 255,150,81"/>
        <HostSiteOps Ops="SOpAttachWeapon" HoldRotation="1"/>
