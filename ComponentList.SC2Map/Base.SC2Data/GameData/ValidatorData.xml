<?xml version="1.0" encoding="us-ascii"?>
<Catalog>
    <CValidatorCombine id="EnergyDrainInitialValidators">
        <Type value="And"/>
        <CombineArray value="EnergyDrainPeriodicValidators"/>
        <CombineArray value="TargetNotIncapacitated"/>
        <CombineArray value="TargetEnergyGE100"/>
        <CombineArray value="NotEnergyDraining"/>
    </CValidatorCombine>
    <CValidatorCombine id="EnergyDrainPeriodicValidators">
        <Type value="And"/>
        <CombineArray value="TargetEnergyGE50"/>
        <CombineArray value="NotDead"/>
    </CValidatorCombine>
    <CValidatorUnitCompareBehaviorCount id="NotEnergyDraining">
        <ResultFailed value="CantTargetAirUnits"/>
        <Compare value="LE"/>
        <Behavior value="TorEnergyDrainTargetBuff"/>
    </CValidatorUnitCompareBehaviorCount>
    <CValidatorUnitCompareVital id="TargetEnergy">
        <WhichUnit Value="Caster"/>
        <Compare value="GE"/>
        <Value value="100"/>
        <Vital value="Energy"/>
    </CValidatorUnitCompareVital>
    <CValidatorUnitCompareVital id="TargetEnergyGE100">
        <Compare value="GE"/>
        <Value value="100"/>
        <Vital value="Energy"/>
    </CValidatorUnitCompareVital>
    <CValidatorUnitCompareVital id="TargetEnergyGE50">
        <Compare value="GE"/>
        <Value value="50"/>
        <Vital value="Energy"/>
    </CValidatorUnitCompareVital>
</Catalog>
