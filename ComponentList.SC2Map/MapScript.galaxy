            if ((gv_difficultyVotes[lv_i] == 0)) {
                gf_RedHeroMenuInit();
                gf_InitTrigRankCodes(lv_i);
            }
            else if ((gv_difficultyVotes[lv_i] > 0)) {
                lv_count += 1;
                if ((gv_difficultyVotes[lv_i] > 3)) {
                    gv_survival += 1;
                    lv_sum += 200;
                }
                else {
                    lv_sum += (100*gv_difficultyVotes[lv_i]);
